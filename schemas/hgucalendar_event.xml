	<column name="perform" type="number" size="1" notnull="notnull"/>
    <column name="eventname" type="varchar" size="30" notnull="notnull"/>
	<column name="place" type="varchar" size="20" notnull="notnull"/>
    <column name="id" type="varchar" size="40" notnull="notnull"/>
	<column name="phone" type="number" size="11" notnull="notnull"/>
	<column name="fare" type="number" size="7"/>
	<column name="runtime" type="number" size="3" />
	<column name="document_srl" type="varchar" size="10" notnull="notnull"/>
	<column name="startdate" type="varchar" size="10" notnull="notnull" />
	<column name="enddate" type="varchar" size="10" notnull="notnull" />
	<column name="starttime" type="varchar" size="5"/>
